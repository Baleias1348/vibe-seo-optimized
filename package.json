{
  "name": "alliance-vacation-web",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:netlify": "npm install --legacy-peer-deps && npm install vite@latest && npm run build",
    "presitebuild": "node -e \"import('fs').then(fs => import('path').then(path => import('./src/utils/generateSitemap.js').then(({ generateSitemap }) => { const sitemap = generateSitemap(); const outputPath = path.join(process.cwd(), 'public'); if (!fs.existsSync(outputPath)) { fs.mkdirSync(outputPath, { recursive: true }); } fs.writeFileSync(path.join(outputPath, 'sitemap.xml'), sitemap); console.log('Sitemap generado exitosamente'); }))).catch(error => { console.error('Error generando sitemap:', error); process.exit(1); })\"",
    "build": "set -e && npm run presitebuild && NODE_OPTIONS='--experimental-vm-modules' NODE_ENV=production npx vite build --config vite.netlify.config.js",
    "preview": "vite preview --port 5173"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.50.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.16.0",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/parser": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.17",
    "esbuild": "0.17.19",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "npm-force-resolutions": "^0.0.10",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.1",
    "postcss-load-config": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "vite": "^6.3.5"
  }
}
